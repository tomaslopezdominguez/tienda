class Decisions implements \Iterator, \Countable
{
public const DECISION_LITERAL = 0;
public const DECISION_REASON = 1;


protected $pool;

protected $decisionMap;



protected $decisionQueue = [];

public function __construct(Pool $pool)
{
$this->pool = $pool;
