if (5 < count($match)) {
$components = [
'scheme' => $match[2],
'authority' => $match[4],
'path' => $match[5]
];
}

if (7 < count($match)) {
$components['query'] = $match[7];
}

if (9 < count($match)) {
$components['fragment'] = $match[9];
}

return $components;
