public function remove($enumerator): void
{
$ord = ($this->enumeration)::get($enumerator)->getOrdinal();
unset($this->map[$ord]);
}






public function removeIterable(iterable $enumerators): void
{
$map = $this->map;
foreach ($enumerators as $enumerator) {
$ord = ($this->enumeration)::get($enumerator)->getOrdinal();
unset($map[$ord]);
}

