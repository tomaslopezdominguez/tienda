if (!$repo instanceof CompositeRepository) {
return [$repo];
}

$repos = [];
foreach ($repo->getRepositories() as $r) {
foreach (self::flattenRepositories($r, $unwrapFilterRepos) as $r2) {
$repos[] = $r2;
}
}

return $repos;
}
