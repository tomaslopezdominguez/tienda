} catch (TransportException $e) {
$hints = HttpDownloader::getExceptionHints($e);
if (null !== $hints && count($hints) > 0) {
foreach ($hints as $hint) {
$result[] = $hint;
}
}

$result[] = '<error>[' . get_class($e) . '] ' . $e->getMessage() . '</error>';
}

if (isset($tlsWarning)) {
$result[] = $tlsWarning;
}

if (count($result) > 0) {
return $result;
}

return true;
}




