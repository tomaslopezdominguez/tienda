{
$cmd = array_merge(
$cmd,
['--non-interactive'],
$this->getCredentialArgs(),
['--', $url]
);

if ($path !== null) {
$cmd[] = $path;
}

return $cmd;
}








protected function getCredentialArgs(): array
{
if (!$this->hasAuth()) {
