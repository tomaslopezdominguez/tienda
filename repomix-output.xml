$analyzedRule = $rule;
$ruleLevel = 1;
$num = 0;
$l1num = 0;
$seen = [];
$learnedLiteral = null;
$otherLearnedLiterals = [];

$decisionId = \count($this->decisions);

$this->learnedPool[] = [];

while (true) {
